<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Wellington Macena</Authors>
    <Company>Wellington Macena</Company>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>1.0.1</Version>
    <Description>Library created in order to make requests and http responses easier using the native C # class WebRequest</Description>
    <Copyright>2021</Copyright>
    <RepositoryUrl>https://github.com/wellingtonmacena/CosmoRequests</RepositoryUrl>
    <PackageTags>HTTP, HTTP, HTTP Requests, Requests,</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="mscorlib">
      <HintPath>..\..\..\..\..\..\Program Files (x86)\Reference Assemblies\Microsoft\Framework\.NETFramework\v4.8\mscorlib.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
